# Production Environment Variables for Vercel
# Add these to your Vercel project's environment variables

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id_here
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
REACT_APP_FIREBASE_APP_ID=your_app_id_here
REACT_APP_FIREBASE_MEASUREMENT_ID=your_measurement_id_here

# Cloudinary Configuration
REACT_APP_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name_here
REACT_APP_CLOUDINARY_API_KEY=your_cloudinary_api_key_here
REACT_APP_CLOUDINARY_API_SECRET=your_cloudinary_api_secret_here
REACT_APP_CLOUDINARY_UPLOAD_PRESET=rental_images

# Google Maps API
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Paystack Configuration
REACT_APP_PAYSTACK_PUBLIC_KEY=your_paystack_public_key_here

# Application Configuration
REACT_APP_API_BASE_URL=https://your-domain.vercel.app
REACT_APP_ENVIRONMENT=production
